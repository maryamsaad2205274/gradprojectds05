from google.colab import files
uploaded = files.upload()

import zipfile, os

zip_name = list(uploaded.keys())[0]
with zipfile.ZipFile(zip_name, "r") as z:
    z.extractall("/content")

DATA_ROOT = "/content/GradLandmarksDataset"
print("DATA_ROOT:", DATA_ROOT)

import os
req = ["processed/images", "processed/labels", "splits/train.txt", "splits/val.txt"]
for p in req:
    path = os.path.join(DATA_ROOT, p)
    print(p, "=>", "OK" if os.path.exists(path) else "MISSING")

